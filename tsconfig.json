    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./module2/src",
    "outDir": "./module2/dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
